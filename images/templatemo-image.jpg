    <script>
      (function() {
      	let widthHeightRate = 1.0;
      		
      	document.querySelectorAll('.png__size-button').forEach(function(buttonOBJ){
      		buttonOBJ.addEventListener('click',function(){
      			document.querySelector('#svg svg').setAttribute('width', this.value);
      			document.querySelector('#svg svg').setAttribute('height', this.value / widthHeightRate);
      
      			svg2imageData(
      				document.querySelector('#svg svg'),
      				// successCallback
      				function(data) {
      					document.getElementById('canvasImage').src = data;
      					//let link = document.getElementById('pngDownloadLing');
      					//link.href = data;
      
      					download(data,'ntf_327.png','image/png');
      				},
      				// errorCallback
      				function(error) {
      					console.log(error);
      					alert('failed to convert');
      				}
      			);
      			
      			
      		})
      	})
      
      	/*
      	document.getElementById('pngImageSize').addEventListener('change', function(){
      		document.querySelector('#svg svg').setAttribute('width', this.value);
      		document.querySelector('#svg svg').setAttribute('height', this.value / widthHeightRate);
      	})
      	*/
      
      	document.addEventListener('DOMContentLoaded', function() {
      		let width = document.querySelector('#svg svg').width.baseVal.value;
      		let height = document.querySelector('#svg svg').height.baseVal.value;
      		widthHeightRate = width / height;
      
      		document.querySelectoer("#svg-area__viewer svg").add('img-thumbnail');
      
      		//document.getElementById('pngImageSize').value = width;
      
      		/*
      		svg2imageData(
      			document.querySelector('#svg svg'),
      			// successCallback
      			function(data) {
      				document.getElementById('canvasImage').src = data;
      				let link = document.getElementById('pngDownloadLing');
      				link.href = data;
      			},
      			// errorCallback
      			function(error) {
      				console.log(error);
      				alert('failed to convert');
      			}
      		);
      		*/
      	});
      
      
      	
      
      
      
      	function svg2imageData(svgElement, successCallback, errorCallback) {
      		var canvas = document.createElement('canvas');
      		canvas.width = svgElement.width.baseVal.value;
      		canvas.height = svgElement.height.baseVal.value;
      
      
      		var ctx = canvas.getContext('2d');
      		var image = new Image();
      
      		image.onload = () => {
      			ctx.drawImage(image, 0, 0, image.width, image.height);
      			successCallback(canvas.toDataURL());
      		};
      
      		image.onerror = (e) => {
      			errorCallback(e);
      		};
      
      		var svgData = new XMLSerializer().serializeToString(svgElement);
      		image.src = 'data:image/svg+xml;charset=utf-8;base64,' + btoa(svgData);
      	}
      
      }());
    </script>
  
